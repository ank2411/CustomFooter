{
  "vendor": "vtex",
  "name": "countdown",
  "version": "0.0.1",
  "title": "Countdown",
  "description": "Countdown component",
  "defaultLocale": "pt-BR",
  "builders": {
    "messages": "1.x",
    "store": "0.x",
    "react": "3.x"
  },
  "settingsSchema": {
    "title": "Ankit",
    "type": "object",
    "access": "public",
    "properties": {
      "recaptchaKey": {
        "title": "captcha",
        "description": "Enter the recaptcha key for token from user & rules",
        "type": "string"
      }
    }
  },
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.css-handles": "0.x",
    "vtex.store-graphql": "2.x",
    "vtex.file-manager-graphql": "0.x",
    "vtex.apps-graphql": "3.x"
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
